{
  "name": "react-wildcat-example",
  "description": "An example Wildcat project.",
  "version": "1.0.0-beta5",
  "dependencies": {
    "babel": "^5.8.29",
    "react-wildcat": "^1.0.0-beta5",
    "rimraf": "^2.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chalk": "^1.1.1",
    "concurrently": "^1.0.0",
    "eslint": "^1.9.0",
    "eslint-config-nfl": "^4.0.2",
    "eslint-plugin-import": "^0.10.0",
    "eslint-plugin-react": "^3.9.0",
    "istanbul": "nfl/istanbul#feature/source-map-resolve",
    "jspm-resolve": "^1.0.6",
    "karma": "^0.13.15",
    "karma-chai-sinon": "^0.1.5",
    "karma-cli": "^0.1.1",
    "karma-coverage": "nfl/karma-coverage#feature/source-lookup",
    "karma-jspm": "nfl/karma-jspm#feature/normalize-override",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.6",
    "mocha": "^2.3.4",
    "protractor": "^2.5.1",
    "request": "^2.65.0",
    "resolve": "^1.1.6",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "optionalDependencies": {
    "phantomjs": "^1.9.18"
  },
  "jspm": {
    "configFile": "system.config.js",
    "dependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.25",
      "core-js": "npm:core-js@^1.2.6",
      "react": "npm:react@^0.14.2",
      "react-dom": "npm:react-dom@^0.14.2",
      "react-helmet": "npm:react-helmet@^2.1.1",
      "react-metrics": "npm:react-metrics@^1.0.0",
      "react-router": "npm:react-router@^1.0.0",
      "react-wildcat-ensure": "npm:react-wildcat-ensure@^1.0.0-beta5",
      "react-wildcat-handoff": "npm:react-wildcat-handoff@^1.0.0-beta5",
      "react-wildcat-prefetch": "npm:react-wildcat-prefetch@^1.0.0-beta5",
      "react-wildcat-radium": "npm:react-wildcat-radium@^1.0.0-beta5"
    },
    "devDependencies": {
      "isomorphic-fetch": "npm:isomorphic-fetch@^2.2.0",
      "react-addons-test-utils": "npm:react-addons-test-utils@^0.14.2",
      "react-test-tree": "npm:react-test-tree@^1.0.0-rc4",
      "react-transform-catch-errors": "npm:react-transform-catch-errors@^1.0.0",
      "react-transform-hmr": "npm:react-transform-hmr@^1.0.1",
      "react-transform-module-hot-stub": "npm:react-transform-module-hot-stub@^1.0.0-beta5",
      "redbox-react": "npm:redbox-react@^1.1.1"
    }
  },
  "scripts": {
    "build": "time npm run compile -- --ignore '**/{shell,e2e,test}/**' && npm run bundle && jspm depcache public/main.js",
    "bundle": "jspm bundle \"public/**/* - [public/**/*]\" bundles/vendor.js --inject",
    "clean": "rimraf bin public reports",
    "compile": "wildcat-babel --copy-files --binary-to-module src --out-dir public",
    "compile:unit": "env BABEL_ENV=test NODE_ENV=production time npm run compile -- --ignore '**/{shell,e2e}/**'",
    "dev": "env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 concurrent 'wildcat' 'wildcat-static-server' 'npm run compile -- --watch'",
    "karma": "karma start karma.config.js",
    "lint": "eslint src/**/*.js",
    "predev": "npm run clean",
    "preprod-static": "npm run clean && env NODE_ENV=production npm run build",
    "pretest:unit": "npm run clean && npm run compile:unit",
    "prod": "env NODE_ENV=production wildcat",
    "prod-local": "env BABEL_ENV=test concurrent 'npm run prod' 'npm run prod-static'",
    "prod-static": "env NODE_ENV=production wildcat-static-server",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:e2e": "node ./shell/e2e/e2e.js",
    "test:unit": "npm run karma",
    "test:unit:watch": "npm run pretest:unit && concurrent 'npm run karma -- --reporters mocha --single-run false' 'env NODE_ENV=production npm run compile -- --watch'",
    "webdriver-update": "./node_modules/protractor/bin/webdriver-manager update"
  }
}
