{
  "name": "react-wildcat-example",
  "description": "An example Wildcat project.",
  "version": "1.0.0-rc3",
  "dependencies": {
    "babel": "^5.8.29",
    "react-wildcat": "1.0.0-rc3",
    "rimraf": "^2.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "concurrently": "^1.0.0",
    "eslint": "^1.9.0",
    "eslint-config-nfl": "^4.0.2",
    "eslint-import-resolver-jspm": "^1.0.0",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.12.0",
    "istanbul": "nfl/istanbul#feature/source-map-resolve",
    "karma": "^0.13.19",
    "karma-chai-sinon": "^0.1.5",
    "karma-cli": "^0.1.1",
    "karma-coverage": "nfl/karma-coverage#feature/source-lookup",
    "karma-jspm": "nfl/karma-jspm#feature/normalize-override",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.6",
    "mocha": "^2.3.4",
    "protractor": "^3.0.0",
    "protractor-console-plugin": "^0.1.1",
    "protractor-istanbul-plugin": "^2.0.0",
    "react-wildcat-test-runners": "1.0.0-rc3",
    "resolve": "^1.1.6",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "optionalDependencies": {
    "phantomjs": "^1.9.18"
  },
  "jspm": {
    "configFile": "system.config.js",
    "dependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.25",
      "core-js": "npm:core-js@^1.2.6",
      "isomorphic-fetch": "npm:isomorphic-fetch@^2.2.0",
      "radium": "npm:radium@^0.15.3",
      "react": "npm:react@^0.14.3",
      "react-dom": "npm:react-dom@^0.14.3",
      "react-helmet": "npm:react-helmet@^2.1.1",
      "react-metrics": "npm:react-metrics@^1.0.1",
      "react-router": "npm:react-router@^1.0.3",
      "react-wildcat-ensure": "npm:react-wildcat-ensure@1.0.0-rc3",
      "react-wildcat-handoff": "npm:react-wildcat-handoff@1.0.0-rc3",
      "react-wildcat-prefetch": "npm:react-wildcat-prefetch@1.0.0-rc3"
    },
    "devDependencies": {
      "react-addons-test-utils": "npm:react-addons-test-utils@^0.14.3",
      "react-test-tree": "npm:react-test-tree@^1.0.0",
      "react-transform-catch-errors": "npm:react-transform-catch-errors@^1.0.1",
      "react-transform-hmr": "npm:react-transform-hmr@^1.0.1",
      "react-transform-module-hot-stub": "npm:react-transform-module-hot-stub@1.0.0-rc3",
      "react-wildcat-hot-reloader": "npm:react-wildcat-hot-reloader@^1.0.0-rc3",
      "redbox-react": "npm:redbox-react@^1.1.1"
    }
  },
  "scripts": {
    "build": "time npm run compile -- --ignore '**/{shell,e2e,test}/**' && npm run bundle",
    "bundle": "jspm bundle \"public/**/* - [public/**/*]\" bundles/vendor.js --inject",
    "clean": "rimraf bin bundles public",
    "compile": "wildcat-babel --copy-files --binary-to-module src --out-dir public",
    "compile:unit": "env BABEL_ENV=test NODE_ENV=production time npm run compile -- --ignore '**/!(*Test).*'",
    "dev": "env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 concurrent 'wildcat' 'wildcat-static-server' 'npm run compile -- --watch'",
    "lint": "eslint src/**/*.js",
    "predev": "npm run clean",
    "preprod-static": "npm run clean && env NODE_ENV=production npm run build",
    "pretest:e2e": "npm run clean",
    "pretest:unit": "npm run clean && npm run compile:unit",
    "prod": "env NODE_ENV=production wildcat",
    "prod-local": "env BABEL_ENV=test concurrent 'npm run prod' 'npm run prod-static'",
    "prod-static": "env NODE_ENV=production wildcat-static-server",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:e2e": "env COVERAGE=e2e wildcat-protractor-runner",
    "test:unit": "env COVERAGE=unit wildcat-karma-runner",
    "webdriver-update": "./node_modules/protractor/bin/webdriver-manager update"
  }
}
