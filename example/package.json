{
  "name": "react-wildcat-example",
  "description": "An example Wildcat project.",
  "version": "3.0.0",
  "dependencies": {
    "babel": "^5.8.29",
    "koa-favicon": "^1.2.1",
    "koa-route": "^2.4.2",
    "react-wildcat": "3.0.0",
    "rimraf": "^2.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^2.0.0",
    "eslint": "^2.7.0",
    "eslint-config-nfl": "^7.0.1",
    "eslint-import-resolver-jspm": "^2.1.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-mocha": "^2.0.0",
    "eslint-plugin-react": "^4.3.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "istanbul": "https://github.com/nfl/istanbul/tarball/6a3222a5bca6e38af456c1c18d724e1daf69533d",
    "karma": "^0.13.22",
    "karma-chai-sinon": "^0.1.5",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-jspm": "https://github.com/nfl/karma-jspm/tarball/b7de8bfe7ea29bac183c8fee23cfceac41ab3729",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "3.1.1",
    "protractor-console": "^2.0.0",
    "protractor-istanbul-plugin": "^2.0.0",
    "react-wildcat-test-runners": "3.0.0",
    "resolve": "^1.1.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "jspm": {
    "configFile": "system.config.js",
    "dependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.38",
      "core-js": "npm:core-js@^2.2.2",
      "isomorphic-fetch": "npm:isomorphic-fetch@^2.2.1",
      "radium": "npm:radium@^0.17.1",
      "react": "npm:react@15.0.1",
      "react-dom": "npm:react-dom@15.0.1",
      "react-helmet": "npm:react-helmet@^3.0.1",
      "react-metrics": "npm:react-metrics@^1.2.1",
      "react-router": "npm:react-router@^2.0.1",
      "react-wildcat-ensure": "npm:react-wildcat-ensure@3.0.0",
      "react-wildcat-handoff": "npm:react-wildcat-handoff@3.0.0",
      "react-wildcat-prefetch": "npm:react-wildcat-prefetch@3.0.0"
    },
    "devDependencies": {
      "enzyme": "npm:enzyme@^2.2.0",
      "react-addons-test-utils": "npm:react-addons-test-utils@15.0.1",
      "react-transform-catch-errors": "npm:react-transform-catch-errors@^1.0.1",
      "react-transform-hmr": "npm:react-transform-hmr@^1.0.1",
      "react-transform-module-hot-stub": "npm:react-transform-module-hot-stub@2.0.0",
      "react-wildcat-hot-reloader": "npm:react-wildcat-hot-reloader@3.0.0",
      "redbox-react": "npm:redbox-react@^1.2.3"
    },
    "overrides": {
      "npm:enzyme@2.2.0": {
        "map": {
          "react/addons": "@empty",
          "react/lib/ExecutionEnvironment": "@empty",
          "react/lib/ReactContext": "@empty"
        }
      },
      "npm:radium@0.17.1": {
        "dependencies": {
          "array-find": "npm:array-find@1.0.0",
          "exenv": "npm:exenv@1.2.0",
          "inline-style-prefixer": "npm:inline-style-prefixer@1.0.3"
        },
        "main": "dist/radium.js",
        "registry": "jspm"
      }
    }
  },
  "scripts": {
    "build": "npm run compile -- --ignore '**/{shell,e2e,test}/**' && npm run bundle",
    "bundle": "jspm bundle \"public/**/* - [public/**/*]\" bundles/vendor.js --inject",
    "clean": "rimraf bin bundles public",
    "compile": "wildcat-babel --copy-files --binary-to-module src --out-dir public",
    "compile:unit": "env BABEL_ENV=test NODE_ENV=production npm run compile -- --ignore '**/!(*Test).*'",
    "dev": "env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 concurrently 'wildcat' 'wildcat-static-server' 'npm run compile -- --watch'",
    "lint": "eslint src/**/*.js",
    "predev": "npm run clean",
    "preprod-static": "npm run clean && env NODE_ENV=production npm run build",
    "pretest:e2e": "npm run clean",
    "pretest:unit": "npm run clean && npm run compile:unit",
    "prod": "env NODE_ENV=production wildcat",
    "prod-local": "env BABEL_ENV=test concurrently 'npm run prod' 'npm run prod-static'",
    "prod-static": "env NODE_ENV=production wildcat-static-server",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:e2e": "env COVERAGE=e2e wildcat-protractor-runner",
    "test:unit": "env COVERAGE=unit wildcat-karma-runner",
    "webdriver-update": "./node_modules/protractor/bin/webdriver-manager update"
  }
}
