{
  "name": "react-wildcat-handoff",
  "version": "5.0.2",
  "description": "Default client/server rendering for react-wildcat",
  "dependencies": {
    "cookie": "^0.3.1",
    "cookies-js": "^1.2.2",
    "history": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "parse-domain": "^0.2.1",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-helmet": "^3.1.0",
    "react-router": "^2.6.0",
    "react-wildcat-hot-reloader": "5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "rimraf": "^2.5.2",
    "webpack": "^2.1.0-beta.25",
    "webpack-node-externals": "^1.3.3"
  },
  "jspm": {
    "format": "cjs",
    "ignore": [
      "lib",
      "test",
      "src/test",
      ".eslintrc",
      ".npmignore",
      "webpack.config.js"
    ],
    "main": false
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist lib",
    "clean-test": "rimraf lib/**/server{*,}.js",
    "compile": "babel src test/stubFixtures.js --out-dir lib && webpack",
    "pretest": "env NODE_ENV=production npm run build",
    "posttest": "npm run clean-test"
  },
  "author": "engineers@nfl.com",
  "license": "MIT"
}
